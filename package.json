    "core-js": "^2.6.5",
    "cube-ui": "~1.12.15",
    "es6-promise": "^4.2.8",
    "hammerjs": "^2.0.8",
    "husky": "^3.0.9",
    "ical-generator": "^1.9.2",
    "moment": "^2.24.0",
    "moment-timezone-data-webpack-plugin": "^1.1.0",
